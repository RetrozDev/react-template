{
  "name": "react-template",
  "version": "1.0.0",
  "description": "A fullstack template for react app ",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env-shell 'concurrently -c #FF79C6,#BD93F9 -t \"HH:mm:ss\" -p \"{name}: \" \"npm:dev-*\"'",
    "dev-front": "cross-env-shell \"cd ./front && npm run dev\"",
    "dev-back": "cross-env-shell \"cd ./back && npm run dev\"",
    "format": "cross-env-shell \"cd ./front && npm run format && cd ../back && npm run format\""
  },
  "author": "Retroz",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "cors": "^2.8.5",
    "qs": "^6.12.0"
  }
}
